{
  "name": "backend",
  "version": "1.0.1",
  "description": "",
  "main": "./src/app.ts",
  "scripts": {
    "start": "tsx ./src/app.ts",
    "dev": "tsx watch ./src/app.ts",
    "db:migrate": "sh -c 'read -p \"Migration Name: \" NAME && npx sequelize-cli migration:generate --name \"$NAME\"'",
    "db:migrate:up": "npx sequelize-cli db:migrate",
    "db:migrate:down": "npx sequelize-cli db:migrate:undo",
    "db:seed:generate": "npx sequelize-cli seed:generate --name \"$NAME\"",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "email": "email dev --dir src/emails --port 3001",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
